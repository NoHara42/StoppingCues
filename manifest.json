{

  "manifest_version": 2,
  "name": "StoppingCues Beta",
  "version": "1.0",

  "description": "Adds a stopwatch to twitter.com",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["timer.js"],
      "css": ["timer-styling.css"]
    }
  ]

}
